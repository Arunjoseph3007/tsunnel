{
  "name": "tsunnel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-server": "esbuild src/server/index.ts --bundle --platform=node --outfile=dist/server.ts",
    "build-agent": "esbuild src/agent/index.ts --bundle --platform=node --outfile=dist/agent.ts",
    "build-mock": "esbuild src/mock/index.ts --bundle --platform=node --outfile=dist/mock.ts",
    "build-all": "npm run build-server && npm run build-agent && npm run build-mock",
    "build": "npm run build-server && npm run build-agent",
    "server": "node dist/server.js",
    "agent": "node dist/agent.js",
    "mock": "node dist/mock.js"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "esbuild": "^0.24.2",
    "typescript": "^5.7.3"
  }
}
